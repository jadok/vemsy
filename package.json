{
  "name": "vemsy",
  "version": "0.1.1",
  "description": "a markdown framework with express.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "scripts": {
    "test": "npm run lint && npm run test:only && npm run cover",
    "test:only": "BABEL_ENV=test nyc ./node_modules/.bin/mocha --require @babel/register -r @babel/core test/**/*.spec.js --recursive",
    "cover": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "build": "cross-env NODE_ENV=development rollup -c",
    "lint": "eslint src test"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jadok/vemsy.git"
  },
  "author": "Quentin <jadok> Jadeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jadok/vemsy/issues"
  },
  "homepage": "https://github.com/jadok/vemsy#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.3",
    "nyc": "^13.1.0",
    "rollup": "^1.0.2",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "dependencies": {
    "deepmerge": "^3.0.0",
    "express": "^4.16.3",
    "fs": "^0.0.1-security",
    "include-all": "^4.0.3",
    "markdown": "^0.5.0",
    "middleware-setup": "^1.1.0",
    "morgan": "^1.9.1",
    "path": "0.12.7",
    "path-to-regexp": "2.2.1",
    "twig": "^1.13.1",
    "typy": "^3.0.0",
    "winston": "^3.0.0"
  }
}
