    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typedock": "^1.0.9",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "benchmark": "2.1.4",
    "express": "4.16.3",
    "fs": "0.0.1-security",
    "include-all": "4.0.3",
    "markdown": "0.5.0",
    "nconf": "0.10.0",
    "node-sass": "4.9.2",
    "path": "0.12.7",
    "path-to-regexp": "2.2.1",
    "twig": "1.12.0"
