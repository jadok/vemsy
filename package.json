  },
  "homepage": "https://github.com/jadok/vemsy#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.5",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "pm2": "^3.0.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
